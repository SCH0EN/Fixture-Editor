<Window x:Class="InterdisciplinairProject.Fixtures.Views.ExportFixtureWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Export Fixture" Height="150" Width="300" WindowStartupLocation="CenterOwner">
    <StackPanel Margin="10">
        <TextBlock Text="Enter new fixture name:" Margin="0,0,0,5"/>
        <TextBox x:Name="NameTextBox" Text="{Binding FixtureName}" Margin="0,0,0,10"/>
        <Button Content="Export" Width="120" HorizontalAlignment="Right"    Click="Submit_Click"/>
    </StackPanel>
</Window>
